$("<%= @assignment.cohort_id %>new-assign-form").on("ajax:success", checkForForm())

function checkForForm() {
  hideAssignmentForm();
  addAssignment();
}

function hideAssignmentForm() {
  $("#<%= @cohort.id %>-assign-new-part").remove()
}

function addAssignment() {
  $("#<%= @cohort.id %>-assign-partial").replaceWith("<%= j (render partial: 'partials/teachers/teachassignindex', object: @cohort )%>")
}
