<div id="<%= @cohort.id %>-assign-partial">
  <% @cohort.assignments.each do |assignment| %>
      <p>
        <%= link_to(assignment.title, dashboard_cohort_assignment_path(@cohort.id, assignment.id) )  %>
      </p>
    <% end %>

  <div>
    <%= link_to("New Assignment", new_dashboard_cohort_assignment_path )  %>
  </div>
  <div>
    <%= link_to("Cohort Dashboard", dashboard_cohort_path(@cohort.id) )  %>
  </div>
</div>